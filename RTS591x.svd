<?xml version="1.0" encoding="utf-8"?>
<device schemaVersion="1.3" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="CMSIS-SVD.xsd">
  <!-- Product & Vendor -->
  <vendor>Realtek Semiconductor Corp.</vendor>
  <vendorID>Realtek SIBG EC</vendorID>
  <name>RTS591x</name>
  <series>RTS591x Series</series>
  <version>1.0</version>
  <description>Real-M300 Arm v8-M Instruction Processor</description>
  <licenseText>
	  The license belong to Realtek Semiconductor Corp. All the rights reserved.
  </licenseText>

  <!-- CPU -->
  <cpu>
    <name>ARMV8MML</name>
    <revision>r0p0</revision>
    <endian>little</endian>
    <mpuPresent>true</mpuPresent>
    <fpuPresent>true</fpuPresent>
    <vtorPresent>true</vtorPresent>
    <nvicPrioBits>3</nvicPrioBits>
    <vendorSystickConfig>false</vendorSystickConfig>

    <!-- SAU -->
    <sauNumRegions>4</sauNumRegions>
    <sauRegionsConfig enabled="true" protectionWhenDisabled="s">
      <region enabled="false" name="SauRegion0">
        <base>0x00000000</base>
        <limit>0x00000000</limit>
        <access>n</access>
      </region>
      <region enabled="false" name="SauRegion1">
        <base>0x00000000</base>
        <limit>0x00000000</limit>
        <access>n</access>
      </region>
      <region enabled="false" name="SauRegion2">
        <base>0x00000000</base>
        <limit>0x00000000</limit>
        <access>n</access>
      </region>
      <region enabled="false" name="SauRegion3">
        <base>0x00000000</base>
        <limit>0x00000000</limit>
        <access>n</access>
      </region>
    </sauRegionsConfig>
  </cpu>

  <!-- byte addressable memory -->
  <addressUnitBits>8</addressUnitBits>
  <!-- bus width is 32 bits -->
  <width>32</width>
  <!-- this is the default size (number of bits) of all peripherals and register that do not define "size" themselves -->
  <size>32</size>
  <!-- default access permission for all subsequent registers -->
  <access>read-write</access>
  <!-- by default all bits of the registers are initialized to 0 on reset -->
  <resetValue>0x00000000</resetValue>
  <!-- by default all 32Bits of the registers are used -->
  <resetMask>0xffffffff</resetMask>

  <!-- Peripherals -->
  <peripherals>
    <!-- SHA2 -->
    <peripheral>
      <!-- Description -->
      <name>SHA2</name>
      <version>1.0</version>
      <description>Lalu SHA2 Engine</description>
      <groupName>SHA2</groupName>
      <headerStructName>SHA2</headerStructName>
      <baseAddress>0x40000000</baseAddress>
      <size>32</size>
      <access>read-write</access>

      <!-- Address Range -->
      <addressBlock>
        <offset>0</offset>
        <size>0x1000</size>
        <usage>registers</usage>
      </addressBlock>

      <!-- Interrup -->
      <interrupt>
        <name>SHA2</name>
        <description>SHA2 Interrupt</description>
        <value>235</value>
      </interrupt>

      <!-- Registers -->
      <registers>
        <!-- CTRL -->
        <register>
          <name>CTRL</name>
          <description>CONTROL REGISTER</description>
          <addressOffset>0x000</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
          <fields>
            <!-- RST -->
            <field>
              <name>RST</name>
              <description>Reset Digest Registers</description>
              <bitRange>[1:1]</bitRange>
              <enumeratedValues>
                <enumeratedValue>
                  <name>DO_NOTHING</name>
                  <description>Do nothing</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>RESET</name>
                  <description>Reset digest registers</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- DMAMD -->
            <field>
              <name>DMAMD</name>
              <description>DMA Mode Selection</description>
              <bitRange>[2:2]</bitRange>
              <enumeratedValues>
                <enumeratedValue>
                  <name>RD_ONLY</name>
                  <description>Select read-only mode</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>COPY</name>
                  <description>Select copy mode</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- BYTEINV -->
            <field>
              <name>BYTEINV</name>
              <description>Inverse Byte Order or Not</description>
              <bitRange>[3:3]</bitRange>
              <enumeratedValues>
                <enumeratedValue>
                  <name>NATIVE</name>
                  <description>Select native byte order</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>INVERSE</name>
                  <description>Select inverse byte order</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- SHAMD -->
            <field>
              <name>SHAMD</name>
              <description>SHA Mode Selection</description>
              <bitRange>[4:4]</bitRange>
              <enumeratedValues>
                <enumeratedValue>
                  <name>SHA224_256</name>
                  <description>Select SHA224/256</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>SHA384_512</name>
                  <description>Select SHA384/512</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- ICGEN -->
            <field>
              <name>ICGEN</name>
              <description>Enable Auto Clock Gating</description>
              <bitRange>[5:5]</bitRange>
              <enumeratedValues>
                <enumeratedValue>
                  <name>DISABLE</name>
                  <description>Disable ICG</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>ENABLE</name>
                  <description>Enable ICG</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- SLVMD -->
            <field>
              <name>SLVMD</name>
              <description>Slave Mode/DMA Mode Selection</description>
              <bitRange>[6:6]</bitRange>
              <enumeratedValues>
                <enumeratedValue>
                  <name>DMA</name>
                  <description>Select DMA mode</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>SLV</name>
                  <description>Select slave mode</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>
        <!-- STS -->
        <register>
          <name>STS</name>
          <description>STATUS REGISTER</description>
          <addressOffset>0x004</addressOffset>
          <size>32</size>
          <access>read-only</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
          <fields>
            <!-- FIFOSPACE -->
            <field>
              <name>FIFOSPACE</name>
              <description>Number Of Available FIFO Space In Byte</description>
              <bitRange>[4:0]</bitRange>
            </field>
            <!-- BUSY -->
            <field>
              <name>BUSY</name>
              <description>Engine Busy Bit</description>
              <bitRange>[6:6]</bitRange>
              <enumeratedValues>
                <enumeratedValue>
                  <name>IDLE</name>
                  <description>Engine is idle</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>BUSY</name>
                  <description>Engine is busy</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>
        <!-- DIGESTn -->
        <register>
          <dim>16</dim>
          <dimIncrement>4</dimIncrement> 
          <name>DIGEST[%s]</name>
          <description>DEGIEST REGISTER</description>
          <addressOffset>0x010</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- FIFOIN -->
        <register>
          <name>FIFOIN</name>
          <description>FIFO INPUT DATA REGISTER</description>
          <addressOffset>0x050</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
      </registers>
    </peripheral>
    <!-- SHA2DMA -->
    <peripheral>
      <!-- Description -->
      <name>SHA2DMA</name>
      <version>1.0</version>
      <description>Lalu SHA2 DMA Controller</description>
      <groupName>SHA2</groupName>
      <headerStructName>SHA2DMA</headerStructName>
      <baseAddress>0x40001000</baseAddress>
      <size>32</size>
      <access>read-write</access>

      <!-- Address Range -->
      <addressBlock>
        <offset>0</offset>
        <size>0x1000</size>
        <usage>registers</usage>
      </addressBlock>

      <!-- Registers -->
      <registers>
        <!-- SAR -->
        <register>
          <name>SAR</name>
          <description>SOURCE ADDRESS REGISTER</description>
          <addressOffset>0x000</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- DAR -->
        <register>
          <name>DAR</name>
          <description>DESTINATION ADDRESS REGISTER</description>
          <addressOffset>0x008</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- CTRLL -->
        <register>
          <name>CTRLL</name>
          <description>CONTROL REGISTER LOW</description>
          <addressOffset>0x018</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
          <fields>
            <!-- INTEN -->
            <field>
              <name>INTEN</name>
              <description>Interrupt Enable Bit</description>
              <bitRange>[0:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>DISABLE</name>
                  <description>Disable interrup</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>ENABLE</name>
                  <description>Enable interrup</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- DSTTRWIDTH -->
            <field>
              <name>DSTTRWIDTH</name>
              <description>Destination Transfer Width</description>
              <bitRange>[3:1]</bitRange>
              <access>read-write</access>
            </field>
          <!-- SRCTRWIDTH -->
            <field>
              <name>SRCTRWIDTH</name>
              <description>Source Transfer Width</description>
              <bitRange>[6:4]</bitRange>
              <access>read-write</access>
            </field>
            <!-- DINC -->
            <field>
              <name>DINC</name>
              <description>Destination Address Increment</description>
              <bitRange>[8:7]</bitRange>
              <access>read-write</access>
            </field>
            <!-- SINC -->
            <field>
              <name>SINC</name>
              <description>Source Address Increment</description>
              <bitRange>[10:9]</bitRange>
              <access>read-write</access>
            </field>
            <!-- DESTMSIZE -->
            <field>
              <name>DESTMSIZE</name>
              <description>Destination Burst Transaction Length</description>
              <bitRange>[13:11]</bitRange>
              <access>read-write</access>
            </field>
            <!-- SRCMSIZE -->
            <field>
              <name>SRCMSIZE</name>
              <description>Source Burst Transaction Length</description>
              <bitRange>[16:14]</bitRange>
              <access>read-write</access>
            </field>
          </fields>
        </register>
        <!-- CTRLH -->
        <register>
          <name>CTRLH</name>
          <description>CONTROL REGISTER HIGH</description>
          <addressOffset>0x1C</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
          <fields>
            <!-- BLOCKTS -->
            <field>
              <name>BLOCKTS</name>
              <description>Block Transfer Size</description>
              <bitRange>[11:0]</bitRange>
              <access>read-write</access>
            </field>
          </fields>
        </register>
        <!-- CFG -->
        <register>
          <name>CFG</name>
          <description>CONFIGURATION REGISTER</description>
          <addressOffset>0x44</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
          <fields>
            <!-- PROTCTL -->
            <field>
              <name>PROTCTL</name>
              <description>Protection Control</description>
              <bitRange>[3:3]</bitRange>
              <access>read-write</access>
            </field>
          </fields>
        </register>
        <!-- MSKTFR -->
        <register>
          <name>MSKTFR</name>
          <description>MASK FOR INTTFR INTERRUPT REGISTER</description>
          <addressOffset>0x310</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- CLRTFR -->
        <register>
          <name>CLRTFR</name>
          <description>CLEAR INTERRUPT STATUS FOR INTTFR INTERRUPT REGISTER</description>
          <addressOffset>0x338</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- INTSTS -->
        <register>
          <name>INTSTS</name>
          <description>COMBINED INTERRUPT SATAUS REGISTER</description>
          <addressOffset>0x360</addressOffset>
          <size>32</size>
          <access>read-only</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
		      <fields>
            <!-- TFR -->
            <field>
              <name>TFR</name>
              <description>OR of The Contents of StatusTfr Register</description>
              <bitRange>[0:0]</bitRange>
            </field>
            <!-- BLOCK -->
            <field>
              <name>BLOCK</name>
              <description>OR of The Contents of StatusBlock Register</description>
              <bitRange>[1:1]</bitRange>
            </field>
            <!-- SRCT -->
            <field>
              <name>SRCT</name>
              <description>OR of The Contents of StatusSrcTran Register</description>
              <bitRange>[2:2]</bitRange>
            </field>
            <!-- DSTT -->
            <field>
              <name>DSTT</name>
              <description>OR of The Contents of StatusDst Register</description>
              <bitRange>[3:3]</bitRange>
            </field>
            <!-- ERR -->
            <field>
              <name>ERR</name>
              <description>OR of The Contents of StatusErr Register</description>
              <bitRange>[4:4]</bitRange>
            </field>
		      </fields>
        </register>
        <!-- CONF -->
        <register>
          <name>CONF</name>
          <description>CONFIGURATION REGISTER</description>
          <addressOffset>0x398</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
		      <fields>
            <!-- EN -->
            <field>
              <name>EN</name>
              <description>Enable DMA Controller</description>
              <bitRange>[0:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>DISABLE</name>
                  <description>Disable DMA Controller</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>ENABLE</name>
                  <description>Enable DMA Controller</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
		      </fields>
        </register>
        <!-- CHEN -->
        <register>
          <name>CHEN</name>
          <description>CHANNEL ENABLE REGISTER</description>
          <addressOffset>0x3A0</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
		      <fields>
            <!-- CHEN -->
            <field>
              <name>CHEN</name>
              <description>Enables DMA Channel</description>
              <bitRange>[0:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>DISABLE</name>
                  <description>Dsiable DMA Channel</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>ENABLE</name>
                  <description>Enables DMA Channel</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- CHENWE -->
            <field>
              <name>CHENWE</name>
              <description>Enables Channel Write Enable</description>
              <bitRange>[8:8]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>DISABLE</name>
                  <description>Dsiable channel write enable</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>ENABLE</name>
                  <description>Enables channel write enable</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
		      </fields>
        </register>
      </registers>
    </peripheral>
    <!-- SHA3 -->
    <peripheral>
      <!-- Description -->
      <name>SHA3</name>
      <version>1.0</version>
      <description>Lalu SHA3 Engine</description>
      <groupName>SHA3</groupName>
      <headerStructName>SHA3</headerStructName>
      <baseAddress>0x40002000</baseAddress>
      <size>32</size>
      <access>read-write</access>

      <!-- Address Range -->
      <addressBlock>
        <offset>0</offset>
        <size>0x1000</size>
        <usage>registers</usage>
      </addressBlock>

      <!-- Interrup -->
      <interrupt>
        <name>SHA3</name>
        <description>SHA3 Interrupt</description>
        <value>236</value>
      </interrupt>

      <!-- Registers -->
      <registers>
        <!-- ADDR -->
        <register>
          <name>ADDR</name>
          <description>START ADDRESS REGISTER</description>
          <addressOffset>0x000</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- LEN -->
        <register>
          <name>LEN</name>
          <description>DATA LENGTH REGISTER</description>
          <addressOffset>0x004</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- SPEC -->
        <register>
          <name>SPEC</name>
          <description>SPECIFICATION REGISTER</description>
          <addressOffset>0x008</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
		      <fields>
            <!-- PREFIX -->
            <field>
              <name>SPEC</name>
              <description>SHA3 Or SHAKE Selection</description>
              <bitRange>[0:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>SHA3</name>
                  <description>Select SHA3</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>SHAKE</name>
                  <description>Select SHAKE</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- SUFFIX -->
            <field>
              <name>SUFFIX</name>
              <description>Suffix Of SHA3 Specification</description>
              <bitRange>[10:8]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>SHAKE128</name>
                  <description>Select SHAKE128</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>SHA3_224</name>
                  <description>Select SHA3-224</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>SHA3_256</name>
                  <description>Select SHA3-256</description>
                  <value>2</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>SHA3_384</name>
                  <description>Select SHA3-384</description>
                  <value>3</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>SHA3_512</name>
                  <description>Select SHA3-512</description>
                  <value>4</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
		      </fields>
        </register>
        <!-- INTDIS -->
        <register>
          <name>INTDIS</name>
          <description>INTERRUPT DISABLE REGISTER</description>
          <addressOffset>0x00c</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- LPMDDIS -->
        <register>
          <name>LPMDDIS</name>
          <description>LOW POWER MODE DISABLE REGISTER</description>
          <addressOffset>0x010</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- ENABLE -->
        <register>
          <name>ENABLE</name>
          <description>ENABLE REGISTER</description>
          <addressOffset>0x14</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
          <fields>
            <!-- EN -->
            <field>
              <name>EN</name>
              <description>Enable SHA3 Engine To Execute Command</description>
              <bitRange>[10:8]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>UPDATE</name>
                  <description>SHA3 engine DMA starts to receive input data</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>FINALIZE</name>
                  <description>SHA3 engine attches padding data and generate HASH data</description>
                  <value>2</value>
                </enumeratedValue>
                <enumeratedValue>
                    <name>NOT_USED</name>
                    <description>Reserved values. Do not use.</description>
                    <isDefault>true</isDefault>
                </enumeratedValue>
              </enumeratedValues>
            </field>
		      </fields>
        </register>
        <!-- CLR -->
        <register>
          <name>CLR</name>
          <description>CLEAR HASH VALUE AND INPUT BUFFER REGISTER</description>
          <addressOffset>0x18</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- CLRINT -->
        <register>
          <name>CLRINT</name>
          <description>CLEAR INTERRUPT REGISTER</description>
          <addressOffset>0x1C</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- SQUEEZE -->
        <register>
          <name>SQUEEZE</name>
          <description>SQUEEZE REGISTER</description>
          <addressOffset>0x20</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- INTSTS -->
        <register>
          <name>INTSTS</name>
          <description>INTERRUPT STATUS REGISTER</description>
          <addressOffset>0x24</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>
        <!-- RDERR -->
        <register>
          <name>RDERRSTS</name>
          <description>READ ERROR STATUS REGISTER</description>
          <addressOffset>0x2C</addressOffset>
